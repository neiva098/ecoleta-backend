  "bugs": {
    "url": "https://github.com/neiva098/ecoleta-backend/issues"
  },
  "homepage": "https://github.com/neiva098/ecoleta-backend#readme",
  "dependencies": {
    "@types/node": "^14.0.11",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "knex": "^0.21.1",
    "sqlite3": "^4.2.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.0"
  }
